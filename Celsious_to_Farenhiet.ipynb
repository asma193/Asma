      "source": [
        "Celsious = [1,2,12,13,5,6]\n",
        "fahrenheit = list(map(lambda celsious:(celsious * 9/5) + 32,Celsious))\n",
        "\n",
        "print(\"Celsious:\",Celsious)\n",
        "print(\"Fahrenheit:\",fahrenheit)"
